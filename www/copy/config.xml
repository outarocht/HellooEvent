<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "com.telecontact.cover"
	versionCode = "886" 
    version   = "4.0.6">
	
	<access origin="*" />
	
	<preference name="phonegap-version" value="3.1.0" />
	
<name>Telecontact</name>

<gap:plugin name="org.apache.cordova.InAppBrowser" />
<gap:plugin name="org.apache.cordova.contacts" />


<gap:plugin name="org.apache.cordova.device" />

<gap:plugin name="com.adobe.plugins.gaplugin" />
<gap:plugin name="com.phonegap.plugins.barcodescanner"/>
<gap:plugin name="nl.x-services.plugins.socialsharing" />

<gap:plugin name="com.phonegap.plugins.pushplugin" />


<preference name="orientation"      value="portrait" />
<preference name="target-device"    value="universal" />
<preference name="fullscreen"       value="false" />

<icon gap:platform="android" height="64" src="res/icon.png" width="64" />
<gap:splash gap:density="ldpi" gap:platform="android" src="res/splash.png" />
<gap:splash gap:density="mdpi" gap:platform="android" src="res/splash.png" />
<gap:splash gap:density="hdpi" gap:platform="android" src="res/splash.png" />
<gap:splash gap:density="xhdpi" gap:platform="android" src="res/splash.png" />


<description>
Telecontact (Les Pages Jaunes du Maroc).
</description>

<author href="http://www.telecontact.ma" email="contact@telecontact.ma">
Les Pages Jaunes du Maroc.
</author>

</widget>