<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="202007" 
	id="app.mobile.hellooEvent" 
	version="1.0.0" 
	versionCode="001" 
	android-versionCode="8.1" 
	xmlns="http://www.w3.org/ns/widgets" 
	xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HellooEvent</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-device" source="npm" spec="2.0.3" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="2.4.3" />
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="5.0.4" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.3.4" />
    <plugin name="cordova-plugin-camera" source="npm" spec="4.1.0" />
    <plugin name="android-camera-permission" source="npm" spec="~1.0.0" />
    <plugin name="cordova-plugin-media-capture" source="npm" spec="~3.0.3" />
    <plugin name="cordova-plugin-webrtc" source="npm" spec="~0.1.1" />
    <plugin name="cordova-plugin-file" source="npm" spec="~6.0.2" />
    <access origin="*" />
    <access origin="http://*" />
    <access origin="https://*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="android-minSdkVersion" value="20" />
    <preference name="android-targetSdkVersion" value="24" />
    <preference name="android-maxSdkVersion" value="28" />
    <preference name="deployment-target" value="7.0" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/res/icon/android/72.png" />
        <icon density="mdpi" src="www/res/icon/android/96.png" />
        <icon density="hdpi" src="www/res/icon/android/144.png" />
        <icon density="xhdpi" src="www/res/icon/android/192.png" />
    </platform>
</widget>
